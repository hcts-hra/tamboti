                                    <xf:itemset nodeset="instance('code-tables')/code-table[code-table-name='date-point-code']/items/item">
                                        <xf:label ref="label"/>
                                        <xf:value ref="value"/>
                                    </xf:itemset>
                                </xf:select1>
                            </xf:group>
                            <xf:group class="text_10 hide-label margin-plus">
                                <xf:input ref=".">
                                    <xf:label>Copyright Date</xf:label>
                                </xf:input>
                            </xf:group>
                        </xf:repeat>
                    </fieldset>
                </xf:group>
            </xf:repeat>
        </fieldset>
    </xf:group>
    <xf:group ref=".[string(mods:physicalDescription) and not(instance('save-data')/mods:extension/ext:template='newspaper-latin' or instance('save-data')/mods:extension/ext:template='newspaper-transliterated')]">
        <fieldset class="level-1">
            <div class="label-box">
                <span class="legend">Physical Description</span>
                <span class="xforms-help">
                    <span onmouseover="XsltForms_browser.show(this, 'hint', true)" onmouseout="XsltForms_browser.show(this, 'hint', false)" class="xforms-hint-icon"/>
                    <div class="xforms-help-value">
                        <xf:output mediatype="application/xhtml+xml" incremental="true" ref="id('hint-code_physicalDescription-compact',instance('code-tables'))/help"/>
                        <xf:output mediatype="application/xhtml+xml" incremental="true" ref="id('hint-code_more-physical-description',instance('code-tables'))/help"/>
                    </div>
                </span>
            </div>
            <xf:repeat nodeset="./mods:physicalDescription" id="physicalDescription-repeat">
                <xf:group ref=".[mods:internetMediaType]">
                    <fieldset class="level-2">
                        <div class="label-box">
                            <span class="legend">Internet Media Type</span>
                            <span class="xforms-help">
                                <span onmouseover="XsltForms_browser.show(this, 'hint', true)" onmouseout="XsltForms_browser.show(this, 'hint', false)" class="xforms-hint-icon"/>
                                <div class="xforms-help-value">
                                    <xf:output mediatype="application/xhtml+xml" incremental="true" ref="id('hint-code_internetMediaType-compact',instance('code-tables'))/help"/>
                                </div>
                            </span>
                        </div>
                        <xf:repeat nodeset="./mods:internetMediaType" id="internetMediaType-repeat">
                            <xf:group class="margin-plus">
                                <xf:select1 ref=".">
                                    <xf:label>Internet Media Type</xf:label>
                                    <xf:itemset nodeset="instance('code-tables')/code-table[code-table-name='internet-media-type-code']/items/item">
                                        <xf:label ref="label"/>
                                        <xf:value ref="value"/>
                                    </xf:itemset>
                                </xf:select1>
                            </xf:group>
                        </xf:repeat>
                    </fieldset>
                </xf:group>
                <xf:group ref=".[mods:extent]">
                    <fieldset class="level-2">
                        <div class="label-box">
                            <span class="legend">Extent</span>
                            <span class="xforms-help">
                                <span onmouseover="XsltForms_browser.show(this, 'hint', true)" onmouseout="XsltForms_browser.show(this, 'hint', false)" class="xforms-hint-icon"/>
                                <div class="xforms-help-value">
                                    <xf:output mediatype="application/xhtml+xml" incremental="true" ref="id('hint-code_extent-compact',instance('code-tables'))/help"/>
                                </div>
                            </span>
                        </div>
                        <xf:repeat nodeset="./mods:extent" id="extent-repeat">
                            <xf:group class="schema_3.5" ref=".[@unit]">
                                <xf:select1 ref="./@unit" selection="open">
                                    <xf:label>Unit <span class="xforms-help">
                                            <span onmouseover="XsltForms_browser.show(this, 'hint', true)" onmouseout="XsltForms_browser.show(this, 'hint', false)" class="xforms-hint-icon"/>
                                            <div class="xforms-help-value">
                                                <xf:output mediatype="application/xhtml+xml" incremental="true" ref="id('hint-code_part-extent-unit',instance('code-tables'))/help"/>
                                            </div>
                                        </span>
                                    </xf:label>
                                    <xf:itemset nodeset="instance('code-tables')/code-table[code-table-name='part-extent-unit-code']/items/item">
                                        <xf:label ref="label"/>
                                        <xf:value ref="value"/>
                                    </xf:itemset>
                                </xf:select1>
                            </xf:group>
                            <xf:group class="hide-label margin-plus text_10">
                                <xf:input ref=".">
                                    <xf:label>Extent</xf:label>
                                </xf:input>
                            </xf:group>
                        </xf:repeat>
                    </fieldset>
                </xf:group>
            </xf:repeat>
        </fieldset>
    </xf:group>
    <xf:group ref=".[mods:location]">
        <fieldset class="level-1">
            <div class="label-box">
                <span class="legend">
                    <span class="legend">URL</span>
                </span>
                <span class="xforms-help">
                    <span onmouseover="XsltForms_browser.show(this, 'hint', true)" onmouseout="XsltForms_browser.show(this, 'hint', false)" class="xforms-hint-icon"/>
                    <div class="xforms-help-value">
                        <xf:output mediatype="application/xhtml+xml" incremental="true" ref="id('hint-code_url-compact',instance('code-tables'))/help"/>
                        <xf:output mediatype="application/xhtml+xml" incremental="true" ref="id('hint-code_more-location',instance('code-tables'))/help"/>
                    </div>
                </span>
                <span class="add-delete-button">
                    <xf:trigger>
                        <xf:label>+</xf:label>
                        <xf:action ev:event="DOMActivate">
                            <xf:insert nodeset="instance('save-data')/mods:location/mods:url" at="index('url-repeat')" position="before" origin="instance('compact-template')/mods:location/mods:url"/>
                        </xf:action>
                    </xf:trigger>
                </span>
            </div>
            <xf:repeat nodeset="./mods:location" id="location-repeat">
                <xf:repeat nodeset="./mods:url" id="url-repeat">
                    <xf:group class="margin-plus">
                        <xf:group class="text_10">
                            <xf:input ref="./@dateLastAccessed">
                                <xf:label>Last Accessed</xf:label>
                            </xf:input>
                        </xf:group>
                        <xf:group class="text_150">
                            <xf:input ref=".">
                                <xf:label>URL</xf:label>
                            </xf:input>
                        </xf:group>
                    </xf:group>
                    <xf:group ref=".[count(../mods:url) &gt; 1]">
                        <br/>
                    </xf:group>
                </xf:repeat>
            </xf:repeat>
        </fieldset>
    </xf:group>
    <xf:group ref=".[mods:identifier]">
        <fieldset class="level-1">
            <div class="label-box">
                <span class="legend">Identifiers</span>
                <span class="xforms-help">
                    <span onmouseover="XsltForms_browser.show(this, 'hint', true)" onmouseout="XsltForms_browser.show(this, 'hint', false)" class="xforms-hint-icon"/>
                    <div class="xforms-help-value">
                        <xf:output mediatype="application/xhtml+xml" incremental="true" ref="id('hint-code_identifier',instance('code-tables'))/help"/>
                        <xf:output mediatype="application/xhtml+xml" incremental="true" ref="id('hint-code_more-identifier',instance('code-tables'))/help"/>
                    </div>
                </span>
                <span class="add-delete-button">
                    <xf:trigger ref=".[mods:identifier]">
                        <xf:label>+</xf:label>
                        <xf:action ev:event="DOMActivate">
                            <xf:insert nodeset="instance('save-data')/mods:identifier" at="index('identifier-repeat')" position="before" origin="instance('compact-template')/mods:identifier"/>
                        </xf:action>
                    </xf:trigger>
                </span>
            </div>
            <xf:repeat nodeset="./mods:identifier" id="identifier-repeat">
                <xf:group class="hide-label margin-plus text_50">
                    <xf:select1 ref="./@type">
                        <xf:label>Type</xf:label>
                        <xf:itemset nodeset="instance('code-tables')/code-table[code-table-name='identifier-type-code']/items/item">
                            <xf:label ref="label"/>
                            <xf:value ref="value"/>
                        </xf:itemset>
                    </xf:select1>
                    <xf:input ref=".">
                        <xf:label/>
                    </xf:input>
                </xf:group>
            </xf:repeat>
        </fieldset>
    </xf:group>
</div>