                                        <xf:group ref="../.[not(@script='Latn')]"/>
                                    </xf:label>
                                    <xf:itemset nodeset="instance('code-tables')/code-table[code-table-name='name-part-type-compact-code']/items/item">
                                        <xf:label ref="label"/>
                                        <xf:value ref="value"/>
                                    </xf:itemset>
                                </xf:select1>
                                <xf:input ref="." class="text_100">
                                    <xf:label/>
                                </xf:input>
                            </xf:group>
                        </xf:repeat>
                    </fieldset>
                </xf:group>
                <xf:group ref=".[mods:role]">
                    <fieldset class="level-2">
