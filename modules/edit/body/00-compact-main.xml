                                        <span onmouseover="XsltForms_browser.show(this, 'hint', true)" onmouseout="XsltForms_browser.show(this, 'hint', false)" class="xforms-hint-icon"/>
                                        <div class="xforms-help-value">
                                            <xf:output mediatype="application/xhtml+xml" incremental="true" ref="id('hint-code_transliteration-global-compact',instance('code-tables'))/help"/>
                                        </div>
                                    </span>
                                </xf:label>
                                <xf:itemset nodeset="instance('code-tables')/code-table[code-table-name='transliteration-short-code']/items/item">
                                    <xf:label ref="label"/>
                                    <xf:value ref="value"/>
                                </xf:itemset>
                            </xf:select1>
                        </xf:group>
                        <xf:repeat nodeset="./mods:namePart">
                            <xf:group class="margin-plus">
                                <xf:select1 ref="./@type">
                                    <xf:label>
                                        <xf:group ref="../.[@script='Latn']">Transliterated<span class="xforms-hint-icon opaque"/>
